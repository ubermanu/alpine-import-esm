{
  "name": "alpine-import-esm",
  "description": "Import function to load ES modules into component x-data",
  "keywords": [
    "alpinejs",
    "es6",
    "module",
    "import"
  ],
  "version": "1.0.1",
  "author": "ubermanu <manu@paca.happy-dev.fr>",
  "repository": "https://github.com/ubermanu/alpine-import-esm.git",
  "license": "ISC",
  "type": "module",
  "main": "dist/alpine-import-module.cjs",
  "module": "mod.js",
  "types": "mod.d.ts",
  "files": [
    "mod.js",
    "mod.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "alpinejs": "^2.8.2"
  },
  "exports": {
    ".": {
      "import": "./mod.js",
      "default": "./mod.js",
      "require": "./dist/alpine-import-module.cjs"
    },
    "./package.json": "./package.json"
  }
